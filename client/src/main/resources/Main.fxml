<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="Infinity" text="Local storage" textAlignment="CENTER">
                            <font>
                                <Font name="System Bold" size="14.0"/>
                            </font>
                        </Label>
                        <TableView VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn prefWidth="75.0" text="C1"/>
                                <TableColumn prefWidth="75.0" text="C2"/>
                            </columns>
                        </TableView>
                        <HBox>
                            <children>
                                <Button mnemonicParsing="false" text="Upload" maxWidth="Infinity"/>
                                <Button mnemonicParsing="false" text="Rename" maxWidth="Infinity"/>
                                <Button mnemonicParsing="false" text="Delete" maxWidth="Infinity"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="Infinity" text="Cloud storage" textAlignment="CENTER">
                            <font>
                                <Font name="System Bold" size="14.0"/>
                            </font>
                        </Label>
                        <TableView VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn prefWidth="75.0" text="C1"/>
                                <TableColumn prefWidth="75.0" text="C2"/>
                            </columns>
                        </TableView>
                        <HBox>
                            <children>
                                <Button mnemonicParsing="false" text="Download" maxWidth="Infinity"/>
                                <Button mnemonicParsing="false" text="Rename" maxWidth="Infinity"/>
                                <Button mnemonicParsing="false" text="Delete" maxWidth="Infinity"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</VBox>
